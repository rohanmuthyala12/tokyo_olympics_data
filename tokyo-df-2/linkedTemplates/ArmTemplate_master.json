{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tokyo-df-2"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"Athelete Http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/rohanmuthyala12/tokyo_olympics_data/blob/main/data/Athletes.csv"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympic126.dfs.core.windows.net/"
		},
		"Coaches_HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/rohanmuthyala12/tokyo_olympics_data/main/data/Coaches.csv"
		},
		"EntriesGender_HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/rohanmuthyala12/tokyo_olympics_data/main/data/EntriesGender.csv"
		},
		"athelete_HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/rohanmuthyala12/tokyo_olympics_data/main/data/Athletes.csv"
		},
		"medals_HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/rohanmuthyala12/tokyo_olympics_data/main/data/Medals.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tokyo-df-2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"Athelete Http_properties_typeProperties_url": {
						"value": "[parameters('Athelete Http_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Coaches_HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('Coaches_HttpServer1_properties_typeProperties_url')]"
					},
					"EntriesGender_HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('EntriesGender_HttpServer1_properties_typeProperties_url')]"
					},
					"athelete_HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('athelete_HttpServer1_properties_typeProperties_url')]"
					},
					"medals_HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('medals_HttpServer1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}